{
    "name": "nottoday",
    "version": "0.0.1",
    "description": "Twitter blocklist assistant.",
    "scripts": {
        "server:start": "NODE_ENV=production node server/app.js",
        "server:watch": "forever --watch --minUptime 2000 --spinSleepTime 5000 --watchDirectory='./server/' server/app.js",
        "server:forever": "forever --minUptime 1000 --spinSleepTime 2000 server/app.js",
        "client:clean": "rm -rf build/client",
        "client:build": "npm run client:clean && webpack --mode production",
        "client:dev": "npm run client:clean && webpack --mode development",
        "client:watch": "npm run client:clean && webpack --mode development --progress --colors --watch"
    },
    "_moduleAliases": {
        "@app": "./server"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cullylarson/nottoday"
    },
    "author": "Cully Larson <cully.larson@gmail.com>",
    "dependencies": {
        "eslint-config-standard-react": "^7.0.2",
        "express": "^4.16.4",
        "module-alias": "^2.2.0",
        "mysql": "^2.16.0",
        "react": "^16.8.5",
        "react-redux": "^6.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
        "@babel/plugin-transform-react-jsx": "^7.3.0",
        "@babel/plugin-transform-runtime": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "copy-webpack-plugin": "^5.0.2",
        "core-js": "^3.0.0",
        "css-loader": "^2.1.1",
        "cssnano": "^4.1.10",
        "eslint": "^5.15.3",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-react": "^7.12.4",
        "forever": "^0.15.3",
        "postcss-calc": "^7.0.1",
        "postcss-cssnext": "^3.1.0",
        "postcss-custom-media": "^7.0.7",
        "postcss-import": "^12.0.1",
        "postcss-import-webpack-resolver": "^1.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-mixins": "^6.2.1",
        "postcss-nested": "^4.1.2",
        "postcss-nested-ancestors": "^2.0.0",
        "postcss-simple-vars": "^5.0.2",
        "react-dom": "^16.8.5",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "standard": "^12.0.1",
        "style-loader": "^0.23.1",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0",
        "webpack-manifest-plugin": "^2.0.4"
    }
}
